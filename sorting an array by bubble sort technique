      "source": [
        "def bubble_sort(array):\n",
        "    n=len(arr)\n",
        "    for i in range(0,n-1):\n",
        "        for j in range(0,n-i-1):\n",
        "            if arr[j]>arr[j+1]:\n",
        "                arr[j],arr[j+1]=arr[j+1],arr[j]\n",
        "arr=array('i',[64,34,25,12,25,45,65,78,89,9,8,56,98,4,1,104,45,65,89,78,87,98,89,45,16,32,])\n",
        "bubble_sort(arr)\n",
        "print(arr)"
      ],
